wybor_gracza1 = input ('Wybierz papier, kamien lub nozyce')
wybor_gracza2 = input ('Wybierz papier, kamien lub nozyce')

if wybor_gracza1 == wybor_gracza2:
    print ('Jest remis')
    
if wybor_gracza1 == 'papier':
    if wybor_gracza2 == 'kamien':
        print ('Gracz 1 wygral')
    
if wybor_gracza1 == 'kamien':
    if wybor_gracza2 == 'nozyce':
        print ('Gracz 1 wygral')
        
if wybor_gracza1 == 'nozyce':
    if wybor_gracza2 == 'papier':
        print ('Gracz 1 wygral')
        
if wybor_gracza1 == 'papier':
    if wybor_gracza2 == 'nozyce':
        print ('Gracz 2 wygral')
        
if wybor_gracza1 == 'kamien':
    if wybor_gracza2 == 'papier':
        print ('Gracz 2 wygral')
        
if wybor_gracza1 == 'nozyce':
    if wybor_gracza2 == 'kamien':
        print ('Gracz 2 wygral')
